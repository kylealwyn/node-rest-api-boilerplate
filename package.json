{
  "name": "node-api-es6-boilerplate",
  "version": "0.7.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel app -d build",
    "clean": "rm -rf build && mkdir build",
    "start": "nodemon index.js",
    "test": "NODE_ENV=test mocha --recursive --reporter spec --compilers js:babel-register tests",
    "test:watch": "nodemon --exec 'npm run test' tests",
    "coverage": "NODE_ENV=test babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive --reporter spec --compilers js:babel-register tests",
    "validate": "npm run test && npm outdated --depth 0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kylealwyn/node-api-es6-boilerplate.git"
  },
  "keywords": [
    "express",
    "es6",
    "restful",
    "api"
  ],
  "author": "Kyle Alwyn <kjalwyn@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kylealwyn/node-api-es6-boilerplate/issues"
  },
  "homepage": "https://github.com/kylealwyn/node-api-es6-boilerplate",
  "dependencies": {
    "babel-core": "^6.18.0",
    "bcrypt": "^0.8.7",
    "body-parser": "^1.15.2",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.1",
    "errorhandler": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "helmet": "^3.1.0",
    "jade": "^1.11.0",
    "jsonwebtoken": "^7.1.0",
    "lodash": "^4.16.4",
    "method-override": "^2.3.6",
    "mongoose": "^4.6.5",
    "morgan": "^1.7.0",
    "multer": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "eslint": "^3.8.1",
    "eslint-plugin-babel": "^4.0.0",
    "faker": "^3.1.0",
    "isparta": "^4.0.0",
    "mocha": "^3.1.2",
    "nodemon": "^1.11.0"
  }
}
